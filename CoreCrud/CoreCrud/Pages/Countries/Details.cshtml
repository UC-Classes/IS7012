@page
@model CoreCrud.Pages.Countries.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<img src="https://www.centcom.mil/Portals/6/Official_CENTCOM_Seal.png?ver=2016-07-26-155712-367" alt="CENTCOM" width="100" height="100" />
<h1 class="display-4">Details of a CENTCOM Country</h1>

<div>
    <h4>Country</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Country.CountryName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Country.CountryName)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Country.ID">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
